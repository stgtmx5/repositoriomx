CREATE VIEW qryjoin AS SELECT docs.Sys_DTCreated AS Sys_DTCreated, docs.Sys_PK AS Sys_PK,doclink.FK_Parent AS Fk_Parent, docs.Words AS Words, docs.Sys_GUID AS Sys_GUID,docs.Sys_TimeStamp AS Sys_TimeStamp, docs.Description AS Description, docs.Name AS Name,docs.TypeData AS TypeData,doclink.Sis AS Sis FROM (docs JOIN doclink ) WHERE (docs.Sys_GUID = doclink.FK_Doc);