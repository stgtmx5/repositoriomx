CREATE TABLE ut_sat_banco (Sys_PK INT(11) NOT NULL auto_increment, Sys_TimeStamp datetime NOT NULL, Sys_GUID VARCHAR(32) NOT NULL, Sys_DTCreated datetime DEFAULT NULL, Sys_User VARCHAR(5) DEFAULT NULL, Sys_LastUser VARCHAR(5) DEFAULT NULL, Sys_Exported BIT DEFAULT NULL, Sys_DTExported datetime DEFAULT NULL, Sys_Info VARCHAR(32) DEFAULT NULL, Sys_RecVer INT NULL, Sys_Lock INT NULL, Sys_Deleted BIT NULL, uf_cve VARCHAR(50) DEFAULT NULL, uf_nombre VARCHAR(255) DEFAULT NULL, uf_descripcion VARCHAR(1000) DEFAULT NULL, PRIMARY KEY(Sys_PK), UNIQUE KEY Sys_GUID (Sys_GUID))

