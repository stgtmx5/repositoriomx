create table foliospolizas (Sys_PK int auto_increment primary key, Sys_TimeStamp DATETIME  NOT NULL, Sys_GUID VARCHAR(32) NOT NULL  UNIQUE, Sys_DTCreated DATETIME not null, Sys_User VARCHAR(5), Sys_LastUser VARCHAR(5), Sys_Exported BOOL, Sys_DTExported DATETIME, ejercicio int not null, periodo int not null, fk_tipo int not null, fe int not null, fet int not null, fp int not null, fpt int not null, fk_poliza int null, foreign key (fk_tipo) references ctipospolizas(id), foreign key (fk_poliza) references cpoliza(sys_pk), unique(ejercicio, fe), unique(ejercicio, fk_tipo, fet), unique(ejercicio, periodo, fp), unique(ejercicio, periodo, fk_tipo, fpt), unique(fk_poliza));